{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node (ES Modules)",
	"compilerOptions": {
		"composite": true,
		"skipLibCheck": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true,
		"checkJs": true
	}
}
